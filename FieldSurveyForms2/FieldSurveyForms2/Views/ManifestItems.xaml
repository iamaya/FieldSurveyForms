<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
					   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
					   x:Class="FieldSurveyForms2.Views.ManifestItems">
  <StackLayout Orientation="Vertical" VerticalOptions="FillAndExpand">
    <Label Text="{Binding ServiceZone_Name}"  TextColor="Blue" HorizontalOptions="Start"/>
    <StackLayout Orientation="Horizontal">
      <Label Text="{Binding UserFirstName}" HorizontalOptions="Start"/>
      <Label Text="{Binding UserLastName}" HorizontalOptions="Start"/>
    </StackLayout>
     <Label Text="{Binding manifeststatus}" HorizontalOptions="Start" />
  </StackLayout>
</ContentView>