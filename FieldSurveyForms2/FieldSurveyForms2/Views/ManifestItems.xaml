<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
					   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
					   x:Class="FieldSurveyForms2.Views.ManifestItems">
  <StackLayout Orientation="Vertical" HorizontalOptions="StartAndExpand" Padding="20,20,20,20">
    <Label Text="{Binding ServiceZone_Name}" YAlign="Center" />
    <StackLayout Orientation="Horizontal">
      <Label Text="{Binding UserFirstName}" YAlign="Center" />
      <Label Text="{Binding UserLastName}" YAlign="Center" />
    </StackLayout>
    <Label Text="{Binding manifeststatus}" YAlign="Center" />
  </StackLayout>
</ContentView>